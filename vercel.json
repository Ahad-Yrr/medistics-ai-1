{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 3,
  "rewrites": [
    {
      "source": "/_next/:path*",
      "destination": "/_next/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}
